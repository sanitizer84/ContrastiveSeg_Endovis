{
    "dataset": "davinci_1",
    "method": "fcn_segmentor",
    "data": {
      "image_tool": "cv2",
      "input_mode": "BGR",
      "num_classes": 8,
      "label_ready": true,
      "label_class_ids": [0, 1, 2, 3, 4, 5, 6, 7],
      "data_dir": "",
      "workers": 8
    },
   "train": {
      "batch_size": 4,
      "data_transformer": {
        "size_mode": "fix_size",
        "input_size": [512, 512],
        "align_method": "only_scale",
        "pad_mode": "only_scale"
      }
    },
    "val": {
      "batch_size": 4,
      "mode": "ss_test",
      "data_transformer": {
        "size_mode": "fix_size",
        "input_size": [512, 512],
        "align_method": "only_scale"
      }
    },
    "test": {
      "batch_size": 4,
      "mode": "ss_test",
      "out_dir": "",
      "data_transformer": {
        "size_mode": "fix_size",
        "input_size": [512, 512],
        "align_method": "only_scale"
      }
    },
    "train_trans": {
      "trans_seq": [],
      "random_brightness": {
        "ratio": 1.0,
        "shift_value": 10
      },
      "random_hflip": {
        "ratio": 0.5,
        "swap_pair": []
      },
      "random_resize": {
        "ratio": 1.0,
        "method": "random",
        "scale_range": [0.5, 2.0],
        "aspect_range": [0.9, 1.1]
      },
      "random_crop":{
        "ratio": 1.0,
        "crop_size": [256, 256],
        "method": "random",
        "allow_outside_center": false
      }
    },
    "val_trans": {
      "trans_seq": []
    },
    "normalize": {
      "div_value": 255.0,
      "mean_value": [0.485, 0.456, 0.406],
      "mean": [0.485, 0.456, 0.406],
      "std": [0.229, 0.224, 0.225]
    },
    "network":{
      "backbone": "hrnet48",
      "multi_grid": [1, 1, 1],
      "model_name": "",
      "bn_type": "torchsyncbn",
      "stride": 8,
      "factors": [[8, 8]],
      "loss_weights": {
        "corr_loss": 0.01,
        "aux_loss": 0.4,
        "seg_loss": 1.0
      }
    },
    "logging": {
      "logfile_level": "info",
      "stdout_level": "info",
      "log_file": "",
      "log_format": "",
      "rewrite": true
    },
    "lr": {
      "base_lr": 0.01,
      "metric": "iters",
      "lr_policy": "lambda_poly",
      "step": {
        "gamma": 0.5,
        "step_size": 100
      },
      "lambda_poly":{
        "power": 0.9999
      },
      "warm_iters":0,      
      "freeze_backbone":false
    },
    "solver": {
      "display_iter": 300,
      "test_interval": 300,
      "max_iters": 3000
    },
    "optim": {
      "optim_method": "sgd",
      "adam": {
        "betas": [0.9, 0.999],
        "eps": 1e-08,
        "weight_decay": 0.0001
      },
      "sgd": {
        "weight_decay": 0.0005,
        "momentum": 0.9,
        "nesterov": false
      }
    },
    "loss": {
      "loss_type": "fs_ce_loss",
      "params": {
        "ce_weight":
        [0.40006584,
        0.9153615,
        0.960438,
        0.9780319,
        0.8489695,
        0.904349,
        0.99622023,
        0.9965638],
        "ce_reduction": "mean",
        "ce_ignore_index": -1
      }
    },
    "contrast": {
      "proj_dim": 256,
      "temperature": 0.07,
      "base_temperature": 0.07,
      "max_samples": 2048,
      "max_views": 16,
      "stride": 8,
      "loss_weight": 1,
      "use_rmi": false,
      "use_lovasz": false,
      "with_memory": true,
      "memory_size": 5000,
      "pixel_update_freq": 10
    }
}
